<h1>New Post</h1>

<%= form_for :post, url: posts_path do |f| %>
    
    <p>
        <%= f.label :title %>
        <%= f.text_field :title %><br>
    </p>
    <p>
        <%= f.label :boby %>
        <%= f.text_area :body %><br>
        
    </p>
    <p>
        <%= f.submit %>
    </p>
    
<% end %>